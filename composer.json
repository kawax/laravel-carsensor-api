{
    "name": "revolution/laravel-carsensor-api",
    "description": "CarSensor API for Laravel",
    "keywords": [
        "carsensor",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2||^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3||^7.0",
        "illuminate/support": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\CarSensor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Revolution\\CarSensor\\tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\CarSensor\\Providers\\CarSensorServiceProvider"
            ],
            "aliases": {
                "CarSensor": "Revolution\\CarSensor\\Facades\\CarSensor"
            }
        }
    }
}
