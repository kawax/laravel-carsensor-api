{
    "name": "revolution/laravel-carsensor-api",
    "description": "CarSensor API for Laravel",
    "keywords": [
        "carsensor",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\CarSensor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Revolution\\CarSensor\\tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\CarSensor\\Providers\\CarSensorServiceProvider"
            ],
            "aliases": {
                "CarSensor": "Revolution\\CarSensor\\Facades\\CarSensorFacade"
            }
        }
    }
}
